<%- include('../partials/head.ejs') %>
<body>
    <nav> 
        <a href='/'>Home</a><br>
        <a href='/flashcards/new'>Add New Flashcard</a><br>
    </nav>
    <h1>All Flashcards</h1>
    <ul>
        <% flashcards.forEach(flashcard => { %>
            <li>
                <a href='/flashcards/<%= flashcard._id %>'><%= flashcard.title %></a>
            </li>
        <% }) %>
    </ul>
</body>
</html>